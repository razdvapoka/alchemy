.home
  width: 100%
  height: 100%
  &Box
    width: 100%
    height: 100%
    position: relative

.menu
  display: flex
  position: absolute
  top: 0
  left: 50%
  border: 1px solid black
  &Item
    cursor: pointer
    display: flex
    align-items: center
    justify-content: center
    height: 50px
    width: 50px
    background-size: contain
    background-position: center
    background-repeat: no-repeat
  &Item + &Item
    border-left: 1px solid black

